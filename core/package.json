    "cookie-parser": "^1.4.4",
    "cors": "^2.8.3",
    "cross-env": "^5.0.1",
    "dotenv-safe": "^6.0.0",
    "express": "^4.15.2",
    "express-validation": "^1.0.2",
    "faker": "^4.1.0",
    "helmet": "^3.5.0",
    "http-status": "^1.0.1",
    "joi": "^10.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.4",
    "method-override": "^3.0.0",
    "migrate-mongoose": "^4.0.0",
    "moment-timezone": "^0.5.35",
    "mongoose": "^5.6.9",
    "mongoose-paginate-v2": "^1.3.0",
    "morgan": "^1.8.1",
    "multiparty": "^4.2.1",
    "nodemailer": "^6.3.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "4.0.0",
    "pm2": "^3.0.0",
