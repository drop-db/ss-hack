{
  "name": "ss-hack",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "main": "src/index.js",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "precommit": "npm run lint",
    "start": "cross-env node src/index.js",
    "dev": "nodemon src/index.js",
    "lint": "eslint src --ignore-pattern internals/scripts",
    "lint:fix": "npm run lint --fix",
    "lint:watch": "npm run lint --watch",
    "test": "cross-env NODE_ENV=development nyc --reporter=html --reporter=text mocha --timeout 20000 --recursive tests",
    "test:unit": "cross-env NODE_ENV=development mocha tests/unit",
    "test:integration": "cross-env NODE_ENV=development mocha --timeout=20000 -- tests/integration  ",
    "test:watch": "cross-env NODE_ENV=test mocha --watch --timeout=20000 tests/unit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "validate": "npm run lint && npm run test",
    "postpublish": "git push --tags",
    "docs": "apidoc -i src -o docs",
    "postdocs": "opn docs/index.html"
  },
  "keywords": [],
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "2.4.3",
