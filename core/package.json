    "eslint-plugin-import": "^2.2.0",
    "husky": "^0.14.3",
    "mocha": "^3.3.0",
    "nodemon": "^1.11.0",
    "nyc": "^12.0.2",
    "opn-cli": "^3.1.0",
    "sinon": "^6.0.0",
    "sinon-chai": "^3.0.0",
    "supertest": "^3.0.0"
  }
